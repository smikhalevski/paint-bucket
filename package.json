{
  "name": "paint-bucket",
  "version": "2.2.1",
  "description": "Highly performant, extensible, and tiny color manipulation library.",
  "type": "module",
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": "./index.js",
    "./core": "./core.js",
    "./utils": "./utils.js",
    "./color-model/cmyk": "./color-model/cmyk/index.js",
    "./color-model/hsl": "./color-model/hsl/index.js",
    "./color-model/hsv": "./color-model/hsv/index.js",
    "./color-model/hwb": "./color-model/hwb/index.js",
    "./color-model/lab": "./color-model/lab/index.js",
    "./color-model/labh": "./color-model/labh/index.js",
    "./color-model/xyz": "./color-model/xyz/index.js",
    "./plugin/cmyk": "./plugin/cmyk/index.js",
    "./plugin/css": "./plugin/css/index.js",
    "./plugin/difference": "./plugin/difference/index.js",
    "./plugin/hsl": "./plugin/hsl/index.js",
    "./plugin/hsv": "./plugin/hsv/index.js",
    "./plugin/hwb": "./plugin/hwb/index.js",
    "./plugin/lab": "./plugin/lab/index.js",
    "./plugin/labh": "./plugin/labh/index.js",
    "./plugin/palette": "./plugin/palette/index.js",
    "./plugin/rgb": "./plugin/rgb/index.js",
    "./plugin/x11": "./plugin/x11/index.js",
    "./package.json": "./package.json"
  },
  "sideEffects": [
    "./index.js",
    "./plugin/cmyk/index.js",
    "./plugin/css/index.js",
    "./plugin/difference/index.js",
    "./plugin/hsl/index.js",
    "./plugin/hsv/index.js",
    "./plugin/hwb/index.js",
    "./plugin/lab/index.js",
    "./plugin/labh/index.js",
    "./plugin/palette/index.js",
    "./plugin/rgb/index.js",
    "./plugin/x11/index.js"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json && cp package.json README.md LICENSE.txt lib/ && cd lib && npm pkg delete scripts devDependencies",
    "clean": "rimraf lib docs coverage",
    "test": "vitest run",
    "perf": "toofast",
    "docs": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smikhalevski/paint-bucket.git"
  },
  "keywords": [
    "color",
    "gradient",
    "decode",
    "hex",
    "rgb",
    "rgba",
    "hsl",
    "hsla",
    "lab",
    "xyz"
  ],
  "author": "Savva Mikhalevski <smikhalevski@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smikhalevski/paint-bucket/issues"
  },
  "homepage": "https://github.com/smikhalevski/paint-bucket#readme",
  "devDependencies": {
    "chroma-js": "^3.1.2",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tinycolor2": "^1.6.0",
    "toofast": "^3.0.3",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
