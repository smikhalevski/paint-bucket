{
  "name": "paint-bucket",
  "version": "1.1.0",
  "description": "Highly performant, extensible, and tiny color manipulation library.",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.build.json && rollup --config rollup.config.js",
    "clean": "rimraf docs gen lib",
    "test": "jest",
    "perf": "toofast perf.js",
    "docs": "typedoc src/main/color-model/* src/main/plugin/* src/main/utils.ts src/main/index.ts"
  },
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smikhalevski/paint-bucket.git"
  },
  "keywords": [
    "color",
    "decode",
    "hex",
    "rgb",
    "rgba",
    "hsl",
    "hsla"
  ],
  "author": "Savva Mikhalevski <smikhalevski@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smikhalevski/paint-bucket/issues"
  },
  "homepage": "https://github.com/smikhalevski/paint-bucket#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/jest": "^29.5.4",
    "chroma-js": "^2.4.2",
    "jest": "^29.6.3",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.1",
    "rollup-plugin-dts": "^6.0.0",
    "tinycolor2": "^1.6.0",
    "toofast": "^2.0.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.24.8",
    "typedoc-custom-css": "github:smikhalevski/typedoc-custom-css#master",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "algomatic": "^3.2.1"
  }
}
